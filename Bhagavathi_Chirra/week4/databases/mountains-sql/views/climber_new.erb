<h1>
  <strong>
    Add Climber
  </strong>
</h1>
<form action="/climbers" method="post">
  <div>
    <strong>First Name:</strong><input type="text" name="first_name">
  </div>
  <div>
    <strong>Last Name:</strong><input type="text" name="second_name">
  </div>
  <div>
    <strong>Description:</strong><input type="text" name="description">
  </div>
  <div>
    <strong>Age Climbed:</strong><input type="text" name="age_climbed">
  </div>
  <div>
    <strong>Alive:</strong><input type="text" name="alive" placeholder="1 or 0">
  </div>
  <div>
    <strong>Nationality:</strong><input type="text" name="nationality">
  </div>
  <div>
    <strong>Image Url:</strong><input type="text" name="image_url">
  </div>
  <br>
<input type="submit" value="Add Climber" class="button">
</form>
